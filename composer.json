{
    "name":        "e-frank/yii2-knockout",
    "description": "ActiveForm replacement. Automatically generates knockoutjs viewmodels from yii ActiveRecords. Allows computed fields and nested forms.",
    "keywords":    ["yii", "knockout"],
    "type":        "yii2-extension",
    "license":     "BSD-3-Clause",
    "authors": [
        {
            "name":     "Elmar Frank",
            "homepage": "http://yii.efrank.at/extensions/knockout"
        }
    ],
    "require": {
        "yiisoft/yii2":                                    "*",
        "e-frank/yii2-base":                               "*",
        "e-frank/yii2-datetimepicker":                     "*",
        "e-frank/yii2-select2":                            "*",
        "e-frank/yii2-moment":                             "*",
        "e-frank/yii2-bootstrap-material-datetime-picker": "*"
    },
    "autoload": {
        "psr-4": { "x1\\knockout\\": "" }
    }
}
